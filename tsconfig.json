{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es2015", "es2016", "es2017"],
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
